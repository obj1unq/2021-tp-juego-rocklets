import wollok.game.*
import materialesYExtras.*
import bob.*

describe "Tests de Bob" {
	
	const piedra = new Piedra (position = game.center())
 
	fixture {
		game.clear()
		game.addVisual(bob)
		gestorDeMateriales.agregarMaterial(piedra)

	}

	test "Agarrar objeto si es material y agregarlo a la coleccion" {
		bob.guardarMaterial(piedra)
		assert.equals([piedra], bob.materialesGuardados())
	}
	
	

}




